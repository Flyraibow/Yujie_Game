{
	"events": [
    {
      "type" : "setValues",
      "values" : {
        "temp.interaction_description" : "function.lab_selfstudy_result"
      }
    },
    {
      "__comment" : "刷新页面",
      "type" : "refresh",
      "refresh_ids" : [
        "interaction_description"
      ]
    },
    {
      "__comment" : "增长value",
      "type" : "setValues",
      "dataList" : "tempdata.study.attributeChange",
      "changes" : [
        {
          "type" : "add",
          "min" : "0",
          "data" : "associate",
          "field" : "value",
          "value" : "tempdata.study.map.attributeChange.(associate.attributeId)"
        }
      ]
    },
    {
      "__comment" : "增长Proficiency",
      "type" : "setValues",
      "changes" : [
        {
          "type" : "add",
          "min" : "0",
          "max" : "gamedata.ProficiencyData.(tempdata.study.proficiencyId).maxValue",
          "data" : "tempdata.study.proficiencyId",
          "field" : "value",
          "value" : "tempdata.study.addProficiency"
        }
      ]
    },
    {
      "__comment" : "刷新页面",
      "type" : "refresh",
      "refresh_ids" : [
        "my_attribute"
      ]
    },
    {
      "__comment" : "开始执行放学后的工作",
      "type" : "wait",
      "until_click" : true
    }
	]
}
